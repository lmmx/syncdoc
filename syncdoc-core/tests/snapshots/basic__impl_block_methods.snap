#![doc = include_str!("../docs/lib.md")]

pub struct Calculator;

use syncdoc::omnidoc;

#[omnidoc(path = "docs")]

impl Calculator {
    pub fn new() -> Self {
        Self
    }

    pub fn add(&self, a: i32, b: i32) -> i32 {
        a + b
    }

    pub fn multiply(&self, x: i32, y: i32) -> i32 {
        x * y
    }

    fn internal_helper(&self, value: i32) -> i32 {
        value * 2
    }
}
