    pub fn fibonacci(n: u64) -> u64 {
        if n <= 1 {
            n
        } else {
            add_numbers(fibonacci(n - 1), fibonacci(n - 2))
        }
    }
    # [doc = include_str ! ("../docs/calculations/add_numbers.md")]
    fn add_numbers(a: u64, b: u64) -> u64 {
        a + b
    }
